{
    "manifest_version": 3,
    "name": "Email Capture",
    "version": "1.0",
    "description": "It will capture the visible email template on milled.com page",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
